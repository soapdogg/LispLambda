(DEFUN UNI (S1 S2)
    (cond   ((null S1) S2)
            ((null S2) S1)
            (T (cond
                ((MEM (car S1) S2)
                    (UNI (cdr S1) S2))
                (T
                    (cons (car S1) (UNI (cdr S1) S2))
                )
            ))
    )
)

(DEFUN MEM (X LIST)
    (cond   ((null LIST) NIL)
            (T (cond    ((= X (car LIST)) T)
                        (T (MEM X (cdr LIST)))
               )
            )
    )
)

(UNI () ())
(UNI ('(1)) ())
(UNI () ('(1 2)))
(UNI ('(1 2 3)) ())
(UNI () ('(4 5 6)))
