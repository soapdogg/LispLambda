(DEFUN UNI (S1 S2)
    (COND   ((null S1) S2)
            ((null S2) S1)
            (T (COND
                ((MEM (car S1) S2)
                    (UNI (CDR S1) S2))
                (T
                    (CONS (car S1) (UNI (CDR S1) S2))
                )
            ))
    )
)

(DEFUN MEM (X LIST)
    (COND   ((null LIST) NIL)
            (T (COND    ((= X (car LIST)) T)
                        (T (MEM X (CDR LIST)))
               )
            )
    )
)

(UNI () ())
(UNI ('(1)) ())
(UNI () ('(1 2)))
(UNI ('(1 2 3)) ())
(UNI () ('(4 5 6)))
